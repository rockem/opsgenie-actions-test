{
    "env": {
        "browser": false,
        "commonjs": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "no-console": "off",
        "max-len": ["error", { "code": 120 }],
        "indent": ["error", 2]
    }
}
